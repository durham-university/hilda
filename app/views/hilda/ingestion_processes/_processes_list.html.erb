<table class="table table-striped">
  <thead>
    <tr>
      <th>Process</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% ingestion_processes.each do |ingestion_process| %>
      <tr>
        <td><%= link_to ingestion_process.to_s, hilda.edit_ingestion_process_path(ingestion_process) %></td>
        <td><span class='graph_status <%= ingestion_process.run_status %>'><%= ingestion_process.run_status %></span></td>
      </tr>
    <% end %>
  </tbody>
</table>
